

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}


model User {
  uid      Int      @id @default(autoincrement())
  email   String   @unique
  username    String
  role    String
  contactNumber String
  Address String
  password String
}

model Product {
  prodId String @id
  name String
  imageUrl String
  category String
  new_price Int
  old_price Int
  sizes Json
  description String
  
}